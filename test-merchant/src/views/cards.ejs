<style>
    table {
        border-collapse: collapse;
    }
    td, th {
        border: 1px solid black;
        padding: 7px;
    }
</style>
<h1>Added Cards</h1>
<a href="https://card.capitalpay.kz?merchantId=663&cashBoxId=13978">Add Card</a>
<table>
    <thead>
    <tr>
        <th>Number</th>
        <th>Id</th>
    </tr>
    </thead>
    <tbody>
    <% cards.forEach(function (card) { %>
        <tr>
            <td><%= card.card_number %></td>
            <td><%= card.card_id %></td>
        </tr>
    <% }) %>
    </tbody>
</table>
